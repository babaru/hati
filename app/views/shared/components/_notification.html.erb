<%- if alert %>
<div class="alert alert-error notification">
  <button type="button" class="close" data-dismiss="alert">&times;</button>
  <%= alert %>
</div>
<% end -%>
<%- if notice %>
<div class="alert alert-success notification">
  <button type="button" class="close" data-dismiss="alert">&times;</button>
  <%= notice %>
</div>
<% end -%>
